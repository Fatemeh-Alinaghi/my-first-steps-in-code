#include <iostream>
using namespace std;

int main()
{
	int n;
	cin>>n;
    int a[n];
    int c = 0;
    float sum = 0;

    for (int i = 0; i < n; i++)
    {
        cin >> a[i];
        if (a[i] < 0)
            continue;
        sum += a[i];
        c++;
        if (sum > 1000)
            break;
    }

    if (c > 0)
    {
        float avg = sum / c;
        cout << "Average: " << avg << endl;
        cout << "Count: " << c << endl;
    }
    else
    {
        cout << "No positive numbers entered!" << endl;
    }

    return 0;
}
