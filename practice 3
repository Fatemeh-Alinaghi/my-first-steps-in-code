#include <iostream>
using namespace std;

int main()
{
    int i;
    int j;
    int x;
    long fact = 1;
    bool isPrime = true;

    cin >> x;

    if (x < 2)
        isPrime = false;

    for (i = 2; i <= x / 2; i++) {
        if (x % i == 0) {
            isPrime = false;
            break;
        }
    }

    if (isPrime) {
        for (j = 1; j <= x; j++)
            fact *= j;

        cout << "factorial: " << fact << endl;
    } else {
        cout << "not prime" << endl;
    }

    return 0;
}
